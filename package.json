{
  "name": "jquery-classlist",
  "type": "commonjs",
  "version": "1.0.4-pre",
  "description": "Re-implementation of jQuery class manipulation methods that utilizes the classList interface",
  "homepage": "https://github.com/mgol/jquery.classList",
  "author": {
    "name": "Michał Gołębiowski-Owczarek",
    "email": "m.goleb@gmail.com"
  },
  "keywords": [
    "jquery",
    "plugin",
    "ecosystem:jquery",
    "jquery-plugin",
    "browser"
  ],
  "main": "dist/jquery.class_list.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mgol/jquery.classList.git"
  },
  "bugs": "https://github.com/mgol/jquery.classList/issues",
  "license": "MIT",
  "files": [
    "dist/jquery.class_list.js",
    "dist/jquery.class_list.min.js",
    "dist/jquery.class_list.min.js.map",
    "LICENSE.txt"
  ],
  "devDependencies": {
    "eslint": "9.39.1",
    "eslint-config-mgol": "0.0.50",
    "globals": "16.5.0",
    "terser": "5.44.1"
  },
  "scripts": {
    "build": "npm run copy && npm run minify",
    "copy": "node scripts/copy.mjs",
    "minify": "terser dist/jquery.class_list.js --config-file terser.config.json --source-map \"base='dist',filename='jquery.class_list.min.js',url='jquery.class_list.min.js.map'\" -o dist/jquery.class_list.min.js",
    "prepublish": "npm run build",
    "test": "eslint ."
  }
